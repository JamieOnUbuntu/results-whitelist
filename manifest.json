{
    "name":    "Results Whitelist",
	"description": "Highlights sites in Google search results that have been whitelisted by the user.",
    "version": "1.0",
	"permissions": [ "storage" ],
	"options_page": "options.html",
    "manifest_version": 2,

    "browser_action": {
        "default_icon": "rw.ico"
    },

    "content_scripts": [{
        "matches": [
            "https://www.google.co.uk/*",
            "https://www.google.com/*"
        ],
        "js":         ["results.js"],
        "run_at":     "document_end",
        "all_frames": false
    }]
}
